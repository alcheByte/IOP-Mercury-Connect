{
  "expo": {
    "name": "Mercury Connect",
    "description": "Libertaria's Mercury Connect. Connect to Mercury dApps and users!",
    "slug": "mercury-connect",
    "privacy": "unlisted",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/Logo.png",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#101010"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "world.libertaria.mercury.connect.mobile"
    },
    "android": {
      "package": "world.libertaria.mercury.connect.mobile"
    }
  }
}
